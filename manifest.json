{
  "manifest_version": 3,
  "name": "Framework Detective",
  "description": "Detect the framework of a website",
  "version": "1.0.0",
  "icons": { "128": "images/icon_128.png" },
  "action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },
  
  "content_scripts":[
    {
      "matches": ["<all_urls>"],
      "js": ["content_script.js"]
    }
  ],
  "permissions": ["activeTab"]
}
